<TextBox x:Class="DataConveyor.Views.WPF.Views.ConveyorViewTextBox"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
         xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
         xmlns:local="clr-namespace:DataConveyor.Views.WPF.Views"
         xmlns:localStyles="clr-namespace:DataConveyor.Views.WPF"
         mc:Ignorable="d" 
         d:DesignHeight="450" 
         d:DesignWidth="800" 
         AllowDrop="True" 
         IsUndoEnabled="True"  
         TextWrapping="NoWrap" 
         BorderThickness="0,0,0,0"  
         Background="{x:Null}" 
         BorderBrush="{x:Null}"
         Style="{DynamicResource EmptyTextBox}">
</TextBox>
